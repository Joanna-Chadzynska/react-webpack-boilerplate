{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb",
        "prettier",
        "plugin:prettier/recommended",
        "prettier/react"
    ],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ],
        "import/no-extraneous-dependencies": 0,
        "react/jsx-filename-extension": 0,
        "react/prop-types": "off"
    }
}
